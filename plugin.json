{
  "pluginName": "下载选中的链接",
  "description": "右键下载选中的链接文件",
  "version": "0.0.1",
  "preload": "preload.js",
  "logo": "down.png",
  "homepage": "https://github.com/me10zyl/utools-toolbox",
  "features": [
    {
      "code": "urldownloader",
      "explain": "下载选中的链接",
      "cmds": [
        {
          "type": "regex",
          "label": "下载选中的链接",
          "match": "/http(s)?://.+/i",
          "minLength": 1,
          "maxLength": 1000
        }
      ]
    }
  ],
  "author": "me10zyl"
}